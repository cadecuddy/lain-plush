// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model LainPlush {
  id           String  @id
  active       Boolean
  title        String
  endTime      Int
  watchCount   Int?
  currentPrice Float
  url          String
  image        String
}
